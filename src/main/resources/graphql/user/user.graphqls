extend type Query {
    getUser(input: GetUserInput): GetUserPayload!
}

extend type Mutation {
    updateUser(input: UpdateUserInput): UpdateUserPayload!
}

input GetUserInput {
    userId: String!
}

type GetUserPayload {
    user: User!
}

input UpdateUserInput {
    userId: String!
    name: String!
    imageUrl: String!
}

type UpdateUserPayload {
    user: User!
}
